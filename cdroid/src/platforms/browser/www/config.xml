<?xml version='1.0' encoding='utf-8'?>
<widget id="com.example.condroid" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>ConDroid</name>
    <description>
        An application to do some basic network testing on the Narrowband and LTE-M networks.
    </description>
    <author email="isa.sand@teliacompany.com" href="#">
        Division X Device Team 
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
